<html>
	<head>
		<title><%= conference.name %> 公告系統</title>
		<link rel="stylesheet" type="text/css" href="/stylesheets/reset.css">
		<link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
		<link rel="stylesheet" type="text/css" href="/stylesheets/admin.css">
		
	</head>


	<body class="admin-bg">
		<div class="row" align="center">
			<h1><%= conference.name %> 座位表公告系統</h1>
		</div>
		<div class="row" align="center">
			<div class="stage admin-bar" align="center" style="width: 100%;">
				公告內容：<input type="text" id="conf_msg" size="50"/>
				<br/>
				密碼：<input type="password" id="pwd" size="50"/>
				<button id="btn_send" class="btn btn-white" onClick="sendConfMsg()">送出</button>
			</div>
		</div>
		<script type="text/javascript" src="/javascripts/jquery-2.1.3.min.js"></script>
		<script type="text/javascript" src="/javascripts/admin.js"></script>

		<% if(gaid !== '') { %>
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			ga('create', '<%= gaid %>', 'auto');
			ga('send', 'pageview');

		</script>
		<% } %>
	</body>


</html>
